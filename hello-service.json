{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
        "name": "hello",
        "labels": {
            "name": "hello"
        }
    },
    "spec": {
        "ports": [
            {
                "name": "web",
                "protocol": "TCP",
                "port": 5432,
                "targetPort": 8080
            }
        ],
        "selector": {
            "name": "hello-openshift"
        },
        "type": "ClusterIP",
        "sessionAffinity": "None"
    }
}
