{
  "apiVersion": "v1",
  "kind": "Pod",
  "metadata": {
    "name": "winpy",
    "labels": {
      "name": "winpy"
    }
  },
  "spec": {
    "containers": [
      {
        "name": "winpy",
        "image": "rajatchopra/winpyserver",
        "imagePullPolicy": "IfNotPresent",
        "ports": [
          {
            "containerPort": 80
          }
        ]
      }
    ],
    "nodeSelector": {
      "beta.kubernetes.io/os": "windows"
    }
  }
}
